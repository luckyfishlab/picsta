<%= simple_form_for(@album) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.hidden_field :user_id %>
    <%= f.hidden_field :folder_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, :class => "btn btn-large btn-primary" %>
  </div>
<% end %>
